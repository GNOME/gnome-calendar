  <download-page rdf:resource="http://download.gnome.org/sources/gnome-calendar/" />
  <bug-database rdf:resource="https://gitlab.gnome.org/GNOME/gnome-calendar/issues" />
  <category rdf:resource="http://api.gnome.org/doap-extensions#apps" />
