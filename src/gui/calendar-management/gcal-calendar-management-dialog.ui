<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GcalCalendarManagementDialog" parent="AdwWindow">
    <property name="title" translatable="yes">Calendar Settings</property>
    <property name="default-width">500</property>
    <property name="default-height">600</property>
    <property name="modal">True</property>
    <property name="destroy_with_parent">True</property>
    <property name="hide-on-close">True</property>
    <child>
      <object class="AdwNavigationView" id="navigation_view">
        <signal name="popped" handler="on_navigation_view_popped_cb" object="GcalCalendarManagementDialog" swapped="no" />
      </object>
    </child>
  </template>
</interface>
